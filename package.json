{
  "name": "mosip-mobileid-sdk",
  "version": "1.0.3",
  "main": "lib\\commonjs\\index",
  "module": "lib\\module\\index",
  "react-native": "src\\index",
  "types": "lib\\typescript\\src\\index.d.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prepare": "bob build"
  },
  "dependencies": {
    "@react-native-community/bob": "^0.17.1",
    "expo": "~45.0.0",
    "expo-camera": "~12.2.0",
    "expo-face-detector": "^11.2.0",
    "expo-status-bar": "~1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/bob": "^0.17.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "typescript": "~4.3.5"
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "description": "mosip mobileid sdk",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mosip/mosip-mobileid-sdk.git"
