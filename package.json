{
  "name": "mosip-mobileid-sdk-test",
  "version": "0.0.5",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint-staged": "lint-staged",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "dependencies": {
    "@react-native-community/bob": "^0.17.1",
    "expo": "~45.0.0",
    "expo-camera": "~12.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-web": "0.17.7"
  },
  "peerDependencies": {
    "expo": "*",
    "expo-camera": "*",
    "expo-modules-core": "*",
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@react-native-community/bob": "^0.17.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "typescript": "~4.3.5"
  },
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "description": "mosip face matching sdk for inji",
  "directories": {
    "lib": "lib"
  },
  "repository": "git+https://github.com/mosip/mosip-mobileid-sdk.git",
  "keywords": [
    "mosip",
    "mobileid",
    "sdk",
    "monobikash"
  ],
  "author": "Monobikash Das",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mosip/mosip-mobileid-sdk/issues"
  },
  "homepage": "https://github.com/mosip/mosip-mobileid-sdk#readme",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
